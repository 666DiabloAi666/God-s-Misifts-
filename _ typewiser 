ğŸ§¬ 2. TactionEngine.ts â€” Core Taction Handler

import { TactionEvent } from './Typewiser.types';

export class TactionEngine {
  private history: TactionEvent[] = [];

  trigger(event: TactionEvent) {
    event.timestamp = new Date().toISOString();
    this.history.push(event);
    console.log('ğŸ¯ Taction Triggered:', event);
    this.route(event);
  }

  private route(event: TactionEvent) {
    switch (event.type) {
      case 'seal.activate':
        console.log('ğŸ” Sigil Seal Activated');
        break;
      case 'bluetooth.scan':
        console.log('ğŸ“¡ Initiating Bluetooth Scan...');
        break;
      case 'bio.verify':
        console.log('ğŸ§¬ Verifying Biometrics...');
        break;
      default:
        console.log('âš ï¸ Unknown Taction Intent:', event.type);
    }
  }
}


