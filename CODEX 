3. Erlang Module â”€ erlang/logic.erl

-module(logic).
-export([trigger_music_drop/0]).

trigger_music_drop() ->
    io:format("ðŸŽµ Erlang hybrid triggered music drop.~n"),
    os:cmd("cargo run --package music_drop_macro"),
    ok.


