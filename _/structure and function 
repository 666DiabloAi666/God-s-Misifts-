✅ bio_gatekeeper.rs (starter)

fn main() {
    println!("🔐 Bio Gatekeeper Active");

    let voice = "mario";
    let eye_scan = true;

    if eye_scan && voice == "mario" {
        println!("✅ Biometric Match. Crown access granted.");
    } else {
        println!("❌ Intruder Detected.");
    }
}


